{"ast":null,"code":"import NavBar from '@/components/NavBar.vue';\nimport ContactBar from '@/components/ContactBar.vue';\nimport { Application, User } from '@/api';\nexport default {\n  name: 'home-page',\n  data() {\n    return {\n      selectUser: {},\n      appList: []\n    };\n  },\n  watch: {\n    selectUser() {\n      this.getUser();\n    }\n  },\n  mounted() {\n    this.getUser();\n  },\n  methods: {\n    updateUser(event) {\n      this.selectUser = event;\n    },\n    async getUser() {\n      let user = await User.objects.who_iam();\n      let params = {\n        user: user.user_id\n      };\n      let data = await Application.objects.filter(params);\n      this.appList = data;\n    }\n  },\n  components: {\n    NavBar,\n    ContactBar\n  }\n};","map":{"version":3,"mappings":"AAmCA,OAAOA,MAAK,MAAO,yBAAwB;AAC3C,OAAOC,UAAS,MAAO,6BAA4B;AACnD,SAASC,WAAW,EAACC,IAAG,QAAS,OAAM;AAEvC,eAAe;EACbC,IAAI,EAAE,WAAW;EAChBC,IAAI,GAAE;IACD,OAAO;MACHC,UAAU,EAAC,CAAC,CAAC;MACbC,OAAO,EAAC;IACZ;EACJ,CAAC;EACDC,KAAK,EAAC;IACFF,UAAU,GAAE;MACR,IAAI,CAACG,OAAO,EAAC;IACjB;EACJ,CAAC;EACDC,OAAO,GAAE;IACL,IAAI,CAACD,OAAO,EAAC;EACjB,CAAC;EACDE,OAAO,EAAC;IACJC,UAAU,CAACC,KAAK,EAAC;MACb,IAAI,CAACP,UAAU,GAACO,KAAI;IACxB,CAAC;IACD,MAAMJ,OAAO,GAAE;MACX,IAAIK,IAAG,GAAK,MAAMX,IAAI,CAACY,OAAO,CAACC,OAAO,EAAC;MACvC,IAAIC,MAAM,GAAC;QACPH,IAAI,EAACA,IAAI,CAACI;MACd;MACA,IAAIb,IAAG,GAAK,MAAMH,WAAW,CAACa,OAAO,CAACI,MAAM,CAACF,MAAM;MACnD,IAAI,CAACV,OAAM,GAAIF,IAAG;IACtB;EACJ,CAAC;EAEDe,UAAU,EAAE;IACVpB,MAAM;IACNC;EACF;AACJ","names":["NavBar","ContactBar","Application","User","name","data","selectUser","appList","watch","getUser","mounted","methods","updateUser","event","user","objects","who_iam","params","user_id","filter","components"],"sourceRoot":"","sources":["/Users/noone/Desktop/it-dj/dj-project/djsite/psuapp/main-app/src/views/MyApplication.vue"],"sourcesContent":["<template>\n    <nav-bar/>\n    <div class = \"container\">\n    <h2> Мои заявки</h2>\n    <table class=\"table table-hover \">\n    <thead>\n        <tr>\n            <th> id </th>\n            <th> ФИО </th>\n            <th> номер телефона</th>\n            <th> должность </th>\n            <th> вид проблемы </th>\n            <th> статус </th>\n            <th> комметарии </th>\n        </tr>\n    </thead>\n        <tbody >\n            <tr v-for=\"app in appList\" :key=\"app.id\" >\n                <td>{{app.id}}</td>\n                <td>{{app.user_name}}</td>\n                <td>{{app.tel? app.tel:'-'}}</td>\n                <td>{{app.position}}</td>\n                <td>{{app.kind_of_problem.naming}}</td>\n                <td>{{app.get_status_display}}</td>\n                <td>{{app.comment? app.comment:'-'}}</td>\n            </tr>\n        </tbody>\n    </table>\n    </div>\n\n    <contact-bar/>\n</template>\n\n\n<script>\nimport NavBar from '@/components/NavBar.vue'\nimport ContactBar from '@/components/ContactBar.vue'\nimport { Application,User } from '@/api'\n\nexport default {\n  name: 'home-page',\n   data(){\n        return {\n            selectUser:{},\n            appList:[]\n        }\n    },\n    watch:{\n        selectUser(){\n            this.getUser()\n        },\n    },\n    mounted(){\n        this.getUser()\n    },\n    methods:{\n        updateUser(event){\n            this.selectUser=event\n        },\n        async getUser(){\n            let user  = await User.objects.who_iam()\n            let params={\n                user:user.user_id\n            }\n            let data =  await Application.objects.filter(params)\n            this.appList = data\n        }\n    },\n\n    components: {\n      NavBar,\n      ContactBar,\n    },\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}
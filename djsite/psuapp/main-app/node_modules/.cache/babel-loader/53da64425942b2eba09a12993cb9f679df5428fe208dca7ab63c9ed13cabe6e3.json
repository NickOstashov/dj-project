td> <button @click=\"deleteArticel(art,idx)\">удалить  </button></td>\n                </tr>\n        </tbody>\n    </table>\n\n    <contact-bar/>\n</template>\n\n\n<script>\nimport NavBar from '@/components/NavBar.vue'\nimport ContactBar from '@/components/ContactBar.vue'\nimport { Application } from '@/api'\n\nexport default {\n  name: 'home-page',\n    data(){\n        return{\n          appList:[],\n        }\n    },\n\n    methods:{\n        getApplications(){\n            this.appList = Application.objects.get()\n        }\n\n    },\n\n    mounted(){\n        this.getApplications()\n    },\n\n    components: {\n      NavBar,\n      ContactBar,\n    }\n}\n\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}